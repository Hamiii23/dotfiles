{
  "layer": "top",
  "position": "top",
  "height": 32,
  "spacing": 6,
  "modules-left": [
    "hyprland/window"
  ],
  "modules-center": [
    "hyprland/language",
    "pulseaudio",
    "hyprland/workspaces",
    "hyprland/mode",
    "custom/media",
    "clock",
    "battery"
  ],
  "modules-right": [
    "network",
    "tray"
  ],
  "hyprland/mode": {
    "format": "<span style=\"italic\">{}</span>"
  },
  "hyprland/window": {
    "format": "{}",
    "max-length": 50,
