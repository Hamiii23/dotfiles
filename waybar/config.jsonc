    }
  },
  "clock": {
    "format": "{:%a, %d %b | %H:%M}",
    "format-alt": "{:%H:%M:%S}",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
